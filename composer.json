{
	"name": "doubleedesign/comet-plugin",
	"type": "wordpress-plugin",
	"authors": [
		{
			"name": "Leesa Ward",
			"email": "leesa@doubleedesign.com.au"
		}
	],
	"autoload": {
		"psr-4": {
			"Doubleedesign\\Comet\\WordPress\\": "src/"
		}
	},
	"require": {
		"doubleedesign/comet-components-core": "*",
		"composer/installers": "^2.0",
		"humanmade/block-supports-extended": "^0.2.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "path",
			"url": "../core",
			"options": {
				"symlink": true
			}
		},
		{
			"type": "vcs",
			"url": "https://github.com/humanmade/block-supports-extended"
		}
	],
	"extra": {
		"installer-paths": {
			"vendor/humanmade/block-supports-extended": [
				"type:wordpress-plugin"
			]
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
